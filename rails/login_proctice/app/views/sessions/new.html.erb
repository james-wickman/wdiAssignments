<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>



<%= form_tag sessions_create_path, method: 'post' do %>
	<%= text_field_tag :email, nil, placeholder: "Email", autofocus:true  %>
	<%= password_field_tag :password, nil, placeholder: "Password"  %>
	<%= submit_tag 'Sign In'  %>
<% end %>