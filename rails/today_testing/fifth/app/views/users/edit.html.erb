<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>



<%= form_for @user do |f|  %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name, autofocus: true %>
	<%= f.label :last_name %>
	<%= f.text_field :last_name %>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>

<% end %>